{
  "AppSettings": {
    "Secret": "THIS IS USED TO SIGN AND VERIFY JWT TOKENS, REPLACE IT WITH YOUR OWN SECRET, IT CAN BE ANY STRING"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Demo.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/v2",
      "shouldFetchRegistry": false,
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "appName": "demo-api",
      "healthCheckUrlPath": "/",
      "hostName": "localhost",
      "instanceId": "http://localhost:5000",
      "port": 5000,
      "statusPageUrlPath": "/"
    }
  },
  "AllowedHosts": "*"
}