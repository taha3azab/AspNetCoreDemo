{
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=Library.db"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "eureka": {
    "client": {
      "serviceUrl": "http://localhost:8761/eureka/v2",
      "shouldFetchRegistry": false,
      "shouldRegisterWithEureka": true,
      "validateCertificates": false
    },
    "instance": {
      "appName": "graphql",
      "healthCheckUrlPath": "/",
      "hostName": "localhost",
      "instanceId": "http://localhost:5001",
      "port": 5001,
      "statusPageUrlPath": "/"
    }
  },
  "AllowedHosts": "*"
}
